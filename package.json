{
  "name": "@birkoo/learnstorybook-desing-system-template",
  "version": "3.40.0",
  "private": false,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "polished": "^4.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.3",
    "styled-components": "^5.2.1",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "start-storybook -p 6006 -s public",
    "build": "cross-env BABEL_ENV=production babel src -d dist",
    "release": "sh ./scripts/release.sh",
    "prepub": "rm -rf dist && cp package*.json dist",
    "pub": "npm publish ./dist",
    "cannary":"auto canary"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/node-logger": "^6.4.14",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.14",
    "auto": "^10.32.6",
    "chromatic": "^6.4.1",
    "cross-env": "^7.0.3",
    "prettier": "^2.5.1"
  },
  "description": "tutorial desing system",
  "main": "index.js",
  "author": "Emre Birkan Kervan <kervan95@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/birkankervan/learnstorybook-design-system.git"
}
